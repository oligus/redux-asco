{
  "extends": "airbnb/base",
  "rules": {
    "no-unused-vars": [2, { "varsIgnorePattern": "RewireAPI" }],
    "indent": [2, 2, {"SwitchCase": 1}],
    "semi": [2, "never"],
    "no-var": 2,
    "react/prefer-stateless-function":0,
    "no-debugger": 2,
    "comma-dangle": [2, "never"],
    "no-class-assign": 0,
    "max-len": [2, 120]
  }
}
